<ivy-module version="2.0">
    <info organisation="ur_tags" module="ur_tags"/>
    <dependencies>


        <!-- jsp api files -->
        <dependency org="org.apache.tomcat" name="jsp-api" rev="6.0.14" transitive="false">
            <artifact name="jsp-api" type="jar"/>
        </dependency>
        
         <!-- servlet api files -->
        <dependency org="org.apache.tomcat" name="servlet-api" rev="6.0.14" transitive="false">
            <artifact name="servlet-api" type="jar"/>
        </dependency>
        
         <!-- el api files -->
        <dependency org="org.apache.tomcat" name="el-api" rev="6.0.14" transitive="false">
            <artifact name="el-api" type="jar"/>
        </dependency>

        <!-- log 4j files -->
        <dependency org="log4j" name="log4j" rev="1.2.15" transitive="false">
            <artifact name="log4j" type="jar"/>
        </dependency>

        
        <!--  checkstyle jar files  -->
        <dependency org="checkstyle" name="checkstyle" rev="5.0" transitive="false">
            <artifact name="checkstyle" type="jar"/>
        </dependency>
    </dependencies>
</ivy-module>
